'''
class Solution(object):
    def mySqrt(self, x):
        """
        :type x: int
        :rtype: int
        """
        if x<1:
            return 0
        if x==1:
            return 1
        y=int(len(x))
        a = x//(10*y)
        while a<x:
            if a**2 <= x and (a+1)**2 >x:
                res = a
