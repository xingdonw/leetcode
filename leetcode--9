class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x<0:
            return False
        elif x!=0 and x%10 == 0:
            return False
        else:
            a=0
            k=x
            while x!=0:
                a = a*10 + x%10
                x=x/10
            if k == a:
                return True
            else:
                return False
