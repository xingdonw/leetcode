# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def zigzagLevelOrder(self, root):
        """
        :type root: TreeNode
        :rtype: List[List[int]]
        """
        stack=[root]
        res=[]
        carry=1
        if root==None:
            return []
        while stack:
            temp=[]
            for i in range (len(stack)):
                top=stack.pop(0)
                temp.append(top.val)
                if top.left:
                    stack.append(top.left)
                if top.right:
                    stack.append(top.right)
            if carry==-1:
                temp.reverse()
            res.append(temp)
            carry=carry*(-1)
        return res
