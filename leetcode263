class Solution(object):
    def isUgly(self, num):
        """
        :type num: int
        :rtype: bool
        """
        while num == 0:
            return False
        while num%2==0 or num%3==0 or num%5==0:
            if num%2==0:
                num=int(num/2)
            elif num%3==0:
                num=int(num/3)
            elif num%5==0:
                num=int(num/5)
        if num==1:
            return True
        else:
            return False
